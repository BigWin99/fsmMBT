<?xml version="1.0" ?>

<fsm name="SBConnect MBT Demo">
	<states>
		<state name="login" validstring="Sign In">
			<transition input="validPass" next="home" />
			<transition input="invalidPass" next="login" />
		</state>
		<state name="home" validstring="Home">
			<transition input="clicklogout" next="login" />
			<transition input="clickhome" next="home" />
			<transition input="clickrolo" next="rolo" />
      <transition input="clickbi" next="bi" />
		</state>
		<state name="rolo" validstring="Other ways">
      <transition input="clicklogout" next="login" />
      <transition input="clickrolo" next="rolo" />
      <transition input="clickhome" next="home" />
      <transition input="clickbi" next="bi" />
		</state>
		<state name="bi" validstring="Open Quickbooks">
      <transition input="clicklogout" next="login" />
      <transition input="clickbi" next="bi" />
      <transition input="clickhome" next="home" />
      <transition input="clickrolo" next="rolo" />
		</state>
	</states>
</fsm>